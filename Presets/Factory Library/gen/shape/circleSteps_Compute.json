	"UNIFORMS_CODE": "//--- label=Parameters\nint Splits; // default=24; min=1; max=64\nint Seed; // default=0; min=0; max=100\nfloat Innerradius; // default=0.6; label=Radius;section=1\nfloat Offset; // default=0.33;min=-0.001;max=1.001;section=1\nfloat Blur; // default=0.001;min=0.001\n\n",
	"FUNCTIONS_CODE": "#include </libs/lygia/space/rotateZ_glsl>\n#include </libs/lygia/generative/random_glsl>\n#include </libs/lygia/sdf/circleSDF_glsl>\n",
	"OUTPUTS_CODE": "vec4 SPACE;\nvec4 SHAPE;",
	"MAIN_CODE": "// Normalized Center Ratio Position\nvec2 pos = _UVNCR;\n\n// Circle SDF \nfloat circlesdf = circleSDF(pos, vec2(0,0));\n// Split Circle\nfloat radSteps = ceil(circlesdf * Splits) / Splits;\n// Get borders \nfloat fractSteps = smoothstep(0,.1, abs(sin(circlesdf * Splits * _PI)));\n// Randomize circle steps\nfloat randSteps = random(radSteps + Seed);\n// Rotate State along Z, angle by randomized circle steps\nvec3 rotPos = rotateZ(vec3(pos, 0), randSteps*6 + (Time.x * randSteps * .5), vec3(0,0,0));\n\n// Get polar coords of rotated circle steps \nfloat atanSteps = abs(atan(rotPos.x, rotPos.y) / _PI);\n// Smooth step each circle step\nfloat offsetSteps = smoothstep(Offset-Blur, Offset+Blur, atanSteps);\n// Draw global circle shape\nfloat finalCircle = smoothstep(Innerradius+.001, Innerradius, ceil(circlesdf * Splits)/Splits);\n\n// Output Store\nstore(SPACE, _UV, td(vec4(rotPos, 0)));\nstore(SHAPE, _UV, td(vec4(offsetSteps * fractSteps * finalCircle)));"
}