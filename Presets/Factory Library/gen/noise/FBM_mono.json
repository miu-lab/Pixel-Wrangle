{
	"codetabs": {
		"inputs": "vec3 Position; // style=Float\nvec3 Scale; // style=Float;\nfloat Freq; // min=0; max=32;\nint Octaves; // min=1; max=16;\nfloat Lacunarity; // min=1.5; max=8;\nfloat Gain; // min=0; max=1;",
		"function": "#include </libs/miu_lab/gen/noise/FBM_glsl>",
		"outputs": "vec4 IMAGE;",
		"main": "vec3 noise;\nvec3 p = _PNCR;\n\n/* Use Input as Coordinates */\nvec4 Input = read(_i0);\np = Input.rgb;\n\n/* Float Noise */\nnoise = vec3(sFBM(p * Scale * Freq + Position, Octaves, Lacunarity, Gain));\n/* Color Noise */\n// noise = sFBMColor(p * Scale * Freq + Position, Octaves, Lacunarity, Gain);\n\n/* Approximative Normalization */\nnoise = noise * 0.8 * 0.5 + 0.5;\n\n/* Write Values */\nwrite(IMAGE, vec4(vec3(noise), 1));\n\n/* Write Values with Input Mask */\n// write(IMAGE, vec4(vec3(noise) * Input.a, Input.a));"
	},
