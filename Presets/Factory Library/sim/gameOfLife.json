	"codetabs": {
		"inputs": "float Radius; // default=0.02;min=0.001;max=.2",
		"function": "#define T(i,j) fetch(_i0, (_P.xy + ivec2(i ,j)), 0).r \n#define N(i,j)  + float( T(i,j) > 0.)\n\nivec2 absp = ivec2(_P.xy);\n\nvec2 p = _UVN;\n\nvec2 mousep = (p-MousePos.xy) * _RATIO2D;\n\nfloat v = fetch(_i0, absp, 0).r;\n\nfloat dist = length(mousep);\n\nfloat n =     N(-1,-1) + N(-1, 0) + N(-1, 1)\n            + N( 0,-1)            + N( 0, 1)\n            + N( 1,-1) + N( 1, 0) + N( 1, 1);\n\n\nfloat col = 0;\nfloat pointer = 0;",
		"outputs": "vec4 IMAGE;",
		"main": "pointer = (1-step(Radius,dist)) * MouseClicks.x;\n\nv += (1.0-float(v > 0.0)) * float(n == 3.0);\nv *= float(n == 2.0) + float(n == 3.0);\n\ncol = v;\ncol += pointer;\n\nwrite(IMAGE, vec4(col));"
	},
